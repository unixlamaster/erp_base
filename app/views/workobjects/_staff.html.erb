<h4>Список сотрудников</h4>

<table>
  <tr>
    <th>ФИО </th>
    <th>Начало</th>
    <th>Окончение</th>
    <th></th>
    <th></th>
  </tr>

<% @wo_staff.each do |st_wo| %>
  <tr>
    <td><%= link_to Staff.find(st_wo.staff_id).fullname , st_wo  if Staff.exists?(st_wo.staff_id)%></td>
    <td><%= st_wo.sdate %></td>
    <td><%= st_wo.edate %></td>
    <td><%= link_to 'Edit', edit_staffobjectjournal_path(st_wo) %></td>
    <td><%= link_to 'Destroy', st_wo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Добавить сотрудника', new_staffobjectjournal_path, staff_id: 1 %>
